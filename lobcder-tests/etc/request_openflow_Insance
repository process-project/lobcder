wget http://www.inmon.com/products/sFlow-RT/sflow-rt.tar.gz
tar -xvzf sflow-rt.tar.gz
cd sflow-rt
screen -d -m sh -c "/opt/sflow-rt/start.sh"</request-schema:postBootScript>
